{
  "name": "nodepop",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "cross-env DEBUG=nodeapi:* nodemon",
    "prod": "cross-env NODE_ENV=production nodemon",
    "cluster": "cross-env NODE_ENV=production node ./bin/cluster",
    "installDB": "cross-env node ./lib/installDB",
    "debub": "node --inspect-brk ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "i18n": "^0.8.4",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "nodemon": "^1.19.4"
  }
}
